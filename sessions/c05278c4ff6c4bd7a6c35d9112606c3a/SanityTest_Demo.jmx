      <JSR223Listener guiclass="TestBeanGUI" testclass="JSR223Listener" testname="JMeterAdvancedErrorLogger" enabled="false">
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="script">// ===================================================
// CONFIGURABLE LOGGER WITH USER DEFINED VARIABLES
// ===================================================

import java.text.SimpleDateFormat

// ---------------- CONFIGURATION FROM USER DEFINED VARIABLES ----------------
def getBaseLogPath() {
    return vars.get(&quot;LOG_BASE_PATH&quot;) ?: &quot;C:/jmeter_nft_phase_4/logs&quot;  // Default: C:/JMeterLogs
}

def getMaxFileSize() {
    return (vars.get(&quot;MAX_FILE_SIZE_KB&quot;) ?: &quot;100&quot;).toInteger()  // Default: 10240KB (10MB equivalent)
}

// ---------------- GLOBAL TIMESTAMP (One for entire test) ----------------
def getTestTimestamp() {
    def timestampKey = &quot;TEST_TIMESTAMP&quot;
    def existingTimestamp = props.get(timestampKey)
    if (existingTimestamp == null) {
        def newTimestamp = new SimpleDateFormat(&quot;dd_MM_yyyy_HHmmss&quot;).format(new Date())
        props.put(timestampKey, newTimestamp)
        return newTimestamp
    }
    return existingTimestamp
}

// ---------------- GET JMX FILENAME ----------------
def getJmxFileName() {
    try {
        def testPlan = ctx.getTestPlan()
        if (testPlan != null) {
            def testPlanName = testPlan.getName()
            if (testPlanName &amp;&amp; testPlanName != &quot;Test Plan&quot;) {
                return testPlanName.replaceAll(&apos;[^a-zA-Z0-9-_]&apos;, &apos;_&apos;)
            }
        }
    } catch (Exception e) {
    }
    return &quot;JMeterTest&quot;
}

// ---------------- GET THREAD LOGGER ----------------
def getThreadLogger() {
    def baseLogPath = getBaseLogPath()
    def maxFileSize = getMaxFileSize()
    def threadName = Thread.currentThread().name
    def sanitizedThreadName = threadName.replaceAll(&apos;[^a-zA-Z0-9-_]&apos;, &apos;_&apos;)
    
    // Get JMX filename and SHARED timestamp
    def jmxName = getJmxFileName()
    def timestamp = getTestTimestamp()  // Same timestamp for all threads
    
    // Create ONE main folder: /JMeterTest_05_10_2025_170712/
    def mainFolder = new File(&quot;${baseLogPath}/${jmxName}_${timestamp}&quot;)
    
    // Create thread folder inside main folder
    def threadFolder = new File(mainFolder, sanitizedThreadName)
    threadFolder.mkdirs()
    
    // Get current file number for this thread
    def fileCounterKey = &quot;FILE_COUNT_${sanitizedThreadName}&quot;
    def currentFileNum = props.get(fileCounterKey) as Integer ?: 1
    
    def logFile = new File(threadFolder, &quot;errors_${currentFileNum}.log&quot;)
    
    // Check if we need to rotate file due to size (now using KB)
    if (logFile.exists() &amp;&amp; logFile.length() &gt; maxFileSize * 1024) {
        currentFileNum++
        props.put(fileCounterKey, currentFileNum)
        logFile = new File(threadFolder, &quot;errors_${currentFileNum}.log&quot;)
    }
    
    return logFile
}

// ---------------- EXTRACT REQUEST BODY ----------------
def extractRequestBody() {
    try {
        def samplerData = prev.getSamplerData()
        if (!samplerData || !samplerData.trim()) {
            return &quot;No request body&quot;
        }
        
        if (samplerData.contains(&quot;POST data:&quot;)) {
            def postDataStart = samplerData.indexOf(&quot;POST data:&quot;) + 10
            def postData = samplerData.substring(postDataStart).trim()
            return postData
        }
        
        if (samplerData.contains(&quot;Query Data:&quot;)) {
            def queryStart = samplerData.indexOf(&quot;Query Data:&quot;) + 11
            def queryData = samplerData.substring(queryStart).trim()
            return queryData
        }
        
        return samplerData
        
    } catch (Exception e) {
        return &quot;Error extracting request body: &quot; + e.getMessage()
    }
}

// ---------------- MAIN LOGGING ----------------
try {
    // Only log failures
    if (prev == null || prev.isSuccessful()) {
        return
    }

    def timestamp = new SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss.SSS&quot;).format(new Date())
    def threadName = Thread.currentThread().name
    def samplerName = prev.getSampleLabel()
    def responseCode = prev.getResponseCode()
    def responseTime = prev.getTime()
    
    // Get thread logger
    def logFile = getThreadLogger()
    
    // Write complete error entry
    logFile.withWriterAppend { writer -&gt;
        writer.writeLine(&quot;=&quot; * 80)
        writer.writeLine(&quot;ERROR - ${timestamp}&quot;)
        writer.writeLine(&quot;Thread: ${threadName} | Sampler: ${samplerName}&quot;)
        writer.writeLine(&quot;Response Code: ${responseCode} | Time: ${responseTime}ms&quot;)
        writer.writeLine(&quot;URL: ${prev.getURL() ?: &apos;N/A&apos;}&quot;)
        writer.writeLine(&quot;=&quot; * 80)
        writer.writeLine(&quot;&quot;)
        
        // 1. REQUEST HEADERS
        writer.writeLine(&quot;REQUEST HEADERS:&quot;)
        writer.writeLine(&quot;-&quot; * 40)
        def reqHeaders = prev.getRequestHeaders()
        if (reqHeaders &amp;&amp; reqHeaders.trim()) {
            writer.writeLine(reqHeaders)
        } else {
            writer.writeLine(&quot;No request headers available&quot;)
        }
        writer.writeLine(&quot;&quot;)
        
        // 2. REQUEST BODY
        writer.writeLine(&quot;REQUEST BODY:&quot;)
        writer.writeLine(&quot;-&quot; * 40)
        def requestBody = extractRequestBody()
        if (requestBody &amp;&amp; requestBody.trim()) {
            writer.writeLine(requestBody)
        } else {
            writer.writeLine(&quot;No request body available&quot;)
        }
        writer.writeLine(&quot;&quot;)
        
        // 3. RESPONSE HEADERS
        writer.writeLine(&quot;RESPONSE HEADERS:&quot;)
        writer.writeLine(&quot;-&quot; * 40)
        def resHeaders = prev.getResponseHeaders()
        if (resHeaders &amp;&amp; resHeaders.trim()) {
            writer.writeLine(resHeaders)
        } else {
            writer.writeLine(&quot;No response headers available&quot;)
        }
        writer.writeLine(&quot;&quot;)
        
        // 4. RESPONSE BODY
        writer.writeLine(&quot;RESPONSE BODY:&quot;)
        writer.writeLine(&quot;-&quot; * 40)
        def resBody = prev.getResponseDataAsString()
        if (resBody &amp;&amp; resBody.trim()) {
            writer.writeLine(resBody)
        } else {
            writer.writeLine(&quot;No response body available&quot;)
        }
        writer.writeLine(&quot;&quot;)
        
        writer.writeLine(&quot;=&quot; * 80)
        writer.writeLine(&quot;&quot;)
    }
    
} catch (Exception e) {
    // Silent fail
}</stringProp>
